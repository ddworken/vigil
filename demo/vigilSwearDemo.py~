import vigil
import math

@vigil.swear(">=", 0, 0)
def square(x):
    return x*x

def main():
    print("When implores are not violated, everything works fine. ")
    print("sqr(2): ")
    print(sqr(2))
    print("But when they are violated...")
    print("sqrt(-2): ")
    print(sqrt(-2))

if __name__ == "__main__":
    main()
